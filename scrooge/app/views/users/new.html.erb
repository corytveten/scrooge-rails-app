<h1>Sign Up</h1>
<%= form_for @user do |f| %>
	<%= f.label :name %><br>
	<%= f.text_field :name %><br><br>
	
	<%= f.label :email %><br>
	<%= f.text_field :email %><br><br>
	
	<%= f.label :user_name %><br>
	<%= f.text_field :user_name %><br><br>
	
	<%= f.label :password %><br>
	<%= f.text_field :password %><br><br>
	
	<%= f.submit 'Create User'%>
	
	<% end %>
