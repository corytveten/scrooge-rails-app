
<%= form_for @transaction do |f| %>

	<%= f.label :category %><br>
	<%= f.text_field :category %><br><br>
	
	<%= f.label :date %><br>
	<%= f.date_field :date %><br><br>
	
	<%= f.label :amount %><br>
	<%= f.number_field :amount %><br><br>
	
	<%= f.label :description %><br>
	<%= f.text_field :description %><br><br>
	
	<%= f.hidden_field :budget_id %>
	
	<%= f.submit %>
	
	<% end %>
	
	
	
	
	
